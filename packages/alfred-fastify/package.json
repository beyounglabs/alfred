{
  "name": "@beyounglabs/alfred-fastify",
  "version": "0.0.13",
  "description": "Alfred JS Framework",
  "scripts": {
    "build": "tsc",
    "publish": "changeset publish"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:beyounglabs/alfred.git",
  "license": "MIT",
  "dependencies": {
    "@beyounglabs/alfred": "workspace:*",
    "@beyounglabs/alfred-apm": "workspace:*",
    "fastify": "^4.20.0",
    "@fastify/swagger": "^8.8.0",
    "ioredis": "^5.2.2",
    "typescript": "5.1.*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {}
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        ".": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "/dist"
  ]
}
