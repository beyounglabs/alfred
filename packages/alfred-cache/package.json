{
  "name": "@beyounglabs/alfred-cache",
  "version": "0.0.9",
  "description": "Alfred JS Framework (Cache)",
  "scripts": {
    "build": "tsc",
    "publish": "changeset publish"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:beyounglabs/alfred.git",
  "license": "MIT",
  "dependencies": {
    "@beyounglabs/alfred": "workspace:*",
    "@beyounglabs/alfred-apm": "workspace:*",
    "@types/lru-cache": "^5.1.0",
    "date-fns": "^1.30.1",
    "ioredis": "^5.2.2",
    "lru-cache": "^5.1.1",
    "memcached": "^2.2.2",
    "moment": "^2.19.2",
    "object-hash": "^3.0.0",
    "typescript": "5.1.*",
    "uniqid": "^4.1.1"
  },
  "devDependencies": {
    "@types/node": "^18"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./.": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "typesVersions": {
    "*": {}
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        ".": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "files": [
    "/dist"
  ]
}
