    "@types/redis": "^2.8.1",
    "@types/validator": "^6.3.0",
    "@types/winston": "^2.3.7",
    "@types/yargs": "^10.0.0",
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "chai": "^4.1.2",
    "dotenv": "^4.0.0",
