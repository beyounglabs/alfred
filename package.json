  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=testing nyc mocha -A --opts ./mocha.opts --require ts-node/register",
    "test:watch": "nodemon -e js,ts  --exec yarn test:clear",
    "test:clear": "clear &&  yarn test",
    "install": "./scripts/install.sh",
    "ts-node": "ts-node",
    "npm:publish": "yarn build && cp package.json dist/ && cp -R scripts dist/ && cp -R defaults dist/  && cd dist/ && /usr/bin/npm publish --access public"
  },
